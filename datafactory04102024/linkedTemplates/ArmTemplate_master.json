{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "datafactory04102024"
		},
		"AzureDataLakeStorageLinkGit_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorageLinkGit'"
		},
		"AzureSynapseAnalyticsDpoolLink_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'AzureSynapseAnalyticsDpoolLink'"
		},
		"AzureDataLakeStorageLinkGit_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://datalakestorage25092024.dfs.core.windows.net/"
		},
		"AzureSynapseAnalyticsDpoolLink_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "synpseworkspace26092024.sql.azuresynapse.net"
		},
		"AzureSynapseAnalyticsDpoolLink_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "synpsedepool_27092024"
		},
		"AzureSynapseAnalyticsDpoolLink_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "sqladminuser"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "datafactory04102024_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureDataLakeStorageLinkGit_accountKey": {
						"value": "[parameters('AzureDataLakeStorageLinkGit_accountKey')]"
					},
					"AzureSynapseAnalyticsDpoolLink_password": {
						"value": "[parameters('AzureSynapseAnalyticsDpoolLink_password')]"
					},
					"AzureDataLakeStorageLinkGit_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorageLinkGit_properties_typeProperties_url')]"
					},
					"AzureSynapseAnalyticsDpoolLink_properties_typeProperties_server": {
						"value": "[parameters('AzureSynapseAnalyticsDpoolLink_properties_typeProperties_server')]"
					},
					"AzureSynapseAnalyticsDpoolLink_properties_typeProperties_database": {
						"value": "[parameters('AzureSynapseAnalyticsDpoolLink_properties_typeProperties_database')]"
					},
					"AzureSynapseAnalyticsDpoolLink_properties_typeProperties_userName": {
						"value": "[parameters('AzureSynapseAnalyticsDpoolLink_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}